       RowBox[{"RandomReal", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", " ", "1"}], "}"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"100", ",", " ", "100", ",", "100"}], "}"}]}], "]"}]}], 
      "}"}], ",", "\[IndentingNewLine]", " ", 
     RowBox[{"PyEvaluate", "[", 
      RowBox[{
       RowBox[{"test", "=", "arr"}], ",", "  ", 
       RowBox[{"TimeConstraint", "\[Rule]", "1"}]}], "]"}]}], 
    "\[IndentingNewLine]", "]"}], "//", "AbsoluteTiming"}], "//", 
